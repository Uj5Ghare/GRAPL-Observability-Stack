            "rawSQL": "WITH hourly_metrics AS (   SELECT     regexp_extract(request_url, '([^?]*)') as endpoint,     date_trunc('hour', from_iso8601_timestamp(timestamp)) as hour,     APPROX_PERCENTILE(target_processing_time, 0.95) as p95_latency,     COUNT(*) as request_count   FROM alb_logs   WHERE request_url LIKE '%/api/%'   GROUP BY 1, 2 )\nSELECT   endpoint,   hour,   p95_latency,   request_count,   p95_latency - LAG(p95_latency) OVER (PARTITION BY endpoint ORDER BY hour) as latency_increase\nFROM hourly_metrics\nORDER BY ABS(latency_increase) DESC NULLS LAST\nLIMIT 20;",
